{
  "env": {
    "browser": true,
    "es2020": true,
    "commonjs": true
  },
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["error", {"argsIgnorePattern": "_unused(_.+)?"}],
    "no-constant-condition": ["error", {"checkLoops":  false}],
    "no-prototype-builtins": "off",
    "semi": "error",
    "eol-last": "error"
  },
  "globals": {
    "NODE_ENV": true,
    "RELEASE_VER": true
  }
}